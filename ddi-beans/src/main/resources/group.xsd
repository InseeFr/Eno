<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="ddi:group:3_3" xmlns:r="ddi:reusable:3_3" xmlns:a="ddi:archive:3_3" xmlns:c="ddi:conceptualcomponent:3_3" xmlns:cm="ddi:comparative:3_3" xmlns:d="ddi:datacollection:3_3" xmlns:l="ddi:logicalproduct:3_3" xmlns:p="ddi:physicaldataproduct:3_3" xmlns:pi="ddi:physicalinstance:3_3" xmlns:m1="ddi:physicaldataproduct_ncube_normal:3_3" xmlns:m2="ddi:physicaldataproduct_ncube_tabular:3_3" xmlns:m3="ddi:physicaldataproduct_ncube_inline:3_3" xmlns:s="ddi:studyunit:3_3" xmlns:pr="ddi:ddiprofile:3_3" targetNamespace="ddi:group:3_3" elementFormDefault="qualified" attributeFormDefault="unqualified">
