<ENOParameters>

    <Pipeline>
        <InFormat>ddi</InFormat>
        <OutFormat>fo</OutFormat>

        <PreProcessing>ddi-multimodal-selection</PreProcessing>

        <PreProcessing>ddi-dereferencing</PreProcessing>
        <PreProcessing>ddi-cleaning</PreProcessing>
        <PreProcessing>ddi-titling</PreProcessing>

        <PostProcessing>fo-mailing</PostProcessing>
        <PostProcessing>fo-tableColumn</PostProcessing>
        <PostProcessing>fo-insertEndQuestion</PostProcessing>
        <PostProcessing>fo-editStructurePages</PostProcessing>
        <PostProcessing>fo-specificTreatment</PostProcessing>
        <PostProcessing>fo-insertCoverPage</PostProcessing>
        <PostProcessing>fo-insertAccompanyingMails</PostProcessing>
    </Pipeline>

    <Parameters>

        <Context>business</Context>

        <Campagne>esem-2019-a00</Campagne>

        <Languages>
            <Language>fr</Language>
        </Languages>

        <BeginQuestion>
            <Identification>true</Identification>
        </BeginQuestion>

        <EndQuestion>
            <ResponseTimeQuestion>true</ResponseTimeQuestion>
            <CommentQuestion>true</CommentQuestion>
        </EndQuestion>

        <fo-parameters>
            <Roster>
                <Row>
                    <DefaultSize>10</DefaultSize>
                    <MinimumEmpty>1</MinimumEmpty>
                </Row>
            </Roster>
            <Loop>
                <DefaultOccurrence>5</DefaultOccurrence>
                <MinimumEmptyOccurrence>1</MinimumEmptyOccurrence>
            </Loop>
            <Capture>
                <Numeric>manual</Numeric>
            </Capture>
            <AccompanyingMail>cnrCOL</AccompanyingMail>
        </fo-parameters>

    </Parameters>

</ENOParameters>
