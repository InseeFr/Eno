<ENOParameters>
    
    <Pipeline>
        <InFormat>ddi</InFormat>
        <OutFormat>js</OutFormat>
        
        <PreProcessing>ddi-dereferencing</PreProcessing>
        <PreProcessing>ddi-cleaning</PreProcessing>
        <PreProcessing>ddi-titling</PreProcessing>
        
        <PostProcessing>js-sortComponents</PostProcessing>
        <PostProcessing>js-insert-generic-questions</PostProcessing>
        <PostProcessing>js-externalizeVariables</PostProcessing>
        <PostProcessing>js-vtlParser</PostProcessing>
    </Pipeline>
    
    <Parameters>
        
        <Context>default</Context>
        
        <Campagne>test</Campagne>
        
        <Languages>
            <Language>fr</Language>
            <Language>en</Language>
        </Languages>
        
        <EndQuestion>
            <ResponseTimeQuestion>false</ResponseTimeQuestion>
            <CommentQuestion>true</CommentQuestion>
        </EndQuestion>
        
        <js-parameters>            
            <FilterDescription>true</FilterDescription>
        </js-parameters>
    </Parameters>
    
</ENOParameters>
