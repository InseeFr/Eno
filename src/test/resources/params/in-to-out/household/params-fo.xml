<ENOParameters>

    <Pipeline>
        <InFormat>ddi</InFormat>
        <OutFormat>fo</OutFormat>
        <PreProcessing>ddi-multimodal-selection</PreProcessing>

        <PreProcessing>ddi-dereferencing</PreProcessing>
        <PreProcessing>ddi-cleaning</PreProcessing>
        <PreProcessing>ddi-titling</PreProcessing>

        <PostProcessing>fo-mailing</PostProcessing>
        <PostProcessing>fo-insertEndQuestion</PostProcessing>
        <PostProcessing>fo-editStructurePages</PostProcessing>
        <PostProcessing>fo-specificTreatment</PostProcessing>
        <PostProcessing>fo-insertCoverPage</PostProcessing>
        <PostProcessing>fo-insertAccompanyingMails</PostProcessing>
    </Pipeline>

    <Parameters>

        <Context>household</Context>

        <Campagne>fpe-2019-x00</Campagne>

        <Languages>
            <Language>fr</Language>
        </Languages>

        <EndQuestion>
            <ResponseTimeQuestion>false</ResponseTimeQuestion>
            <CommentQuestion>true</CommentQuestion>
        </EndQuestion>

        <fo-parameters>
            <Format>
                <Orientation>0</Orientation>
                <Columns>2</Columns>
            </Format>
            <Roster>
                <Row>
                    <DefaultSize>8</DefaultSize>
                    <MinimumEmpty>0</MinimumEmpty>
                </Row>
            </Roster>
            <Loop>
                <DefaultOccurrence>8</DefaultOccurrence>
                <MinimumEmptyOccurrence>0</MinimumEmptyOccurrence>
            </Loop>
            <Capture>
                <Numeric>optical</Numeric>
            </Capture>
            <AccompanyingMail>relanceCOLEM</AccompanyingMail>
        </fo-parameters>

        <lunatic-xml-parameters/>

        <fodt-parameters/>

        <Numerotation>
            <QuestNum>all</QuestNum>
        </Numerotation>
    </Parameters>

</ENOParameters>
