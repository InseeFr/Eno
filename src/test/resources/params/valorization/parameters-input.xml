<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ENOParameters>
    <Pipeline>
        <InFormat>ddi</InFormat>
        <OutFormat>fodt</OutFormat>
        <PreProcessing>ddi-dereferencing</PreProcessing>
        <PreProcessing>ddi-cleaning</PreProcessing>
        <PreProcessing>ddi-titling</PreProcessing>
        <PostProcessing>ddi-markdownToXhtml</PostProcessing>
    </Pipeline>
    <Parameters>
        <Context>household</Context>
        <Campagne>test</Campagne>
        <Languages>
            <Language>de</Language>
            <Language>fr</Language>
            <Language>en</Language>
        </Languages>        
        <BeginQuestion>
            <Identification>false</Identification>
        </BeginQuestion>
        <EndQuestion>
            <ResponseTimeQuestion>false</ResponseTimeQuestion>
            <CommentQuestion>true</CommentQuestion>
        </EndQuestion>
        
        <xforms-parameters>
            
            <NumericExample>true</NumericExample>
            <Deblocage>false</Deblocage>
            <Satisfaction>false</Satisfaction>
            <LengthOfLongTable>56</LengthOfLongTable>
            <DecimalSeparator>,</DecimalSeparator>
        </xforms-parameters>
        <fo-parameters>
            <Format>
                <Orientation>90</Orientation>
            </Format>
            <Roster>
                <Row>
                    <DefaultSize>12</DefaultSize>
                </Row>
            </Roster>
            <TextArea>
                <Row>
                    <DefaultSize>3</DefaultSize>
                </Row>
            </TextArea>
            <Table>
                <Row>
                    <DefaultSize>150</DefaultSize>
                </Row>
            </Table>
            <Capture>
                <Numeric>manual</Numeric>
            </Capture>
            <PageBreakBetween>
                <pdf>module</pdf>
            </PageBreakBetween>
            <AccompanyingMail>cnrCOL</AccompanyingMail>            
        </fo-parameters>
        
        <Numerotation>
            <SeqNum>false</SeqNum>
        </Numerotation>
    </Parameters>
</ENOParameters>
