<?xml version="1.0" encoding="UTF-8"?>
<project name="enoFodt" basedir=".">

	<property name="out-extension" value="fodt"/>
	<property name="out-folder" value="fodt"/>

	<!-- fodtPostProcessing : Only copying the basic-form.tmp to the target folder and renaming as form -->
	<target name="odtPostProcessing">
		<copy file="${postprocessing-input-file}"
			tofile="${target-folder}/${survey-name}/${form-name}/form/form.${out-extension}"/>
	</target>
</project>
