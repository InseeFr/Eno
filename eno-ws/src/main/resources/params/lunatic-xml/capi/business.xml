<?xml version="1.0" encoding="UTF-8"?>
<ENOParameters>
    
    <Pipeline>
        <InFormat>ddi</InFormat>
        <OutFormat>lunatic-xml</OutFormat>

		<PreProcessing>ddi-multimodal-selection</PreProcessing>
        <PreProcessing>ddi-markdownToXhtml</PreProcessing>
        <PreProcessing>ddi-dereferencing</PreProcessing>
        <PreProcessing>ddi-cleaning</PreProcessing>
        <PreProcessing>ddi-titling</PreProcessing>

        <PostProcessing>lunatic-xml-sortComponents</PostProcessing>
        <PostProcessing>lunatic-xml-insert-generic-questions</PostProcessing>
        <PostProcessing>lunatic-xml-externalizeVariables</PostProcessing>
        <PostProcessing>lunatic-xml-insertCleaningBlock</PostProcessing>
        <PostProcessing>lunatic-xml-pagination</PostProcessing>
        <PostProcessing>lunatic-xml-specificTreatment</PostProcessing>

    </Pipeline>
    <Mode>capi</Mode>

    <Parameters>
        <Context>household</Context>
        
        <Languages>
            <Language>fr</Language>
        </Languages>
        
        <EndQuestion>
            <CommentQuestion>true</CommentQuestion>
        </EndQuestion>
        
        <Numerotation> 
            <QuestNum>no-number</QuestNum>
        </Numerotation>
        
  </Parameters>
    
</ENOParameters>
